{
    "name": "m-adamski/php-cloudflare-zones-scanner",
    "description": "The tool creates a list of DNS entries for all domains assigned in Cloudflare",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Maciej Adamski",
            "email": "m.adamski@ideacode.pl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "phpoffice/phpspreadsheet": "^1.9",
        "symfony/console": "^4.3"
    },
    "require-dev": {
        "kint-php/kint": "^3.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
